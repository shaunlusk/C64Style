{
  "name": "C64Style",
  "version": "0.2.1",
  "description": "A library for rendering Commodore 64 style graphics with JavaScript and HTML5 Canvas.",
  "main": "index.js",
  "repository": "https://github.com/shaunlusk/c64Style.git",
  "author": "Shaun Lusk <shaun.lusk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run test && npm run build:docs && webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "test": "node ./node_modules/jest/bin/jest ./test --colors --coverage",
    "build:docs": "node ./node_modules/jsdoc/jsdoc.js -R DOCSREADME.MD -d docs ./src/"
  },
  "dependencies": {
    "slcommon": "github:shaunlusk/slcommon",
    "slgfx": "github:shaunlusk/slgfx"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.0.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "jest": "^24.7.1",
    "jsdoc": "^3.5.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.2.3"
  }
}
